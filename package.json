{
  "name": "evortex-front-app",
  "main": "expo-router/entry",
  "scripts": {
    "start": "cross-env NODE_ENV=development expo start",
    "startM": "NODE_ENV=development expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "dev": "cross-env NODE_ENV=development expo start",
    "optimize": "node ./scripts/optimize-build.js",
    "android": "cross-env NODE_ENV=development expo run:android",
    "ios": "cross-env NODE_ENV=development expo run:ios",
    "web": "cross-env NODE_ENV=development expo start --web",
    "prebuild": "cross-env NODE_ENV=development npx expo prebuild",
    "build:android:preview:local": "npm run optimize && cross-env NODE_ENV=production eas build --platform android --profile preview --local",
    "build:android:preview": "npm run optimize && cross-env NODE_ENV=production eas build --platform android --profile preview",
    "update:android:preview": "eas update --branch preview --platform android --clear-cache",
    "build:android": "npm run optimize && cross-env NODE_ENV=production eas build --platform android --profile production",
    "update:android": "eas update --branch production --platform android --clear-cache",
    "build:ios:local": "npm run optimize && cross-env NODE_ENV=production eas build --platform ios --profile production --local",
    "build:ios": "npm run optimize && cross-env NODE_ENV=production eas build --platform ios --profile production",
    "update:ios": "eas update --branch production --platform ios --clear-cache",
    "restore-scripts": "node restore-scripts.js",
    "postinstall": "patch-package",
    "test": "cross-env NODE_ENV=test jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@tanstack/react-query": "^5.72.1",
    "axios": "^1.8.4",
    "babel-plugin-module-resolver": "^5.0.2",
    "clsx": "^2.1.1",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.11.13",
    "expo": "~53.0.23",
    "expo-application": "^6.1.5",
    "expo-device": "~7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.1",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-local-authentication": "~16.0.5",
    "expo-location": "~18.1.6",
    "expo-network": "~7.1.5",
    "expo-notifications": "~0.31.4",
    "expo-router": "~5.1.7",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "~0.30.10",
    "expo-system-ui": "~5.0.11",
    "lodash": "^4.17.21",
    "lottie-react-native": "7.2.2",
    "nativewind": "^4.2.1",
    "patch-package": "^8.0.0",
    "react": "19.0.0",
    "react-native": "0.79.6",
    "react-native-edge-to-edge": "1.6.0",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-modalize": "^2.1.1",
    "react-native-pager-view": "6.7.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.2.1",
    "react-native-view-shot": "^4.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^3.4.17",
    "zod": "^3.24.3",
    "zustand": "4.3.9"
  },
  "resolutions": {
    "zustand": "4.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.16",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "eslint": "^8",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.10",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "18.3.1",
    "typescript": "~5.8.3"
  },
  "private": true
}
