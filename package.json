{
  "name": "evortex-front-app",
  "main": "expo-router/entry",
  "scripts": {
    "start": "cross-env NODE_ENV=development expo start",
    "startM": "NODE_ENV=development expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "dev": "cross-env NODE_ENV=development expo start",
    "optimize": "node ./scripts/optimize-build.js",
    "android": "cross-env NODE_ENV=development expo run:android",
    "ios": "cross-env NODE_ENV=development expo run:ios",
    "web": "cross-env NODE_ENV=development expo start --web",
    "prebuild": "cross-env NODE_ENV=development npx expo prebuild",
    "build:android:preview:local": "npm run optimize && cross-env NODE_ENV=production eas build --platform android --profile preview --local",
    "build:android:preview": "npm run optimize && cross-env NODE_ENV=production eas build --platform android --profile preview",
    "update:android:preview": "eas update --branch preview --platform android --clear-cache",
    "build:android": "npm run optimize && cross-env NODE_ENV=production eas build --platform android --profile production",
    "update:android": "eas update --branch production --platform android --clear-cache",
    "build:ios:local": "npm run optimize && cross-env NODE_ENV=production eas build --platform ios --profile production --local",
    "build:ios": "npm run optimize && cross-env NODE_ENV=production eas build --platform ios --profile production",
    "update:ios": "eas update --branch production --platform ios --clear-cache",
    "restore-scripts": "node restore-scripts.js",
    "postinstall": "patch-package",
    "test": "cross-env NODE_ENV=test jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
